{
  "name": "feline-blog-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "ts-node": {
    "esm": true
  },
  "scripts": {
    "dev": "dotenv -e .env.development -- next dev --turbopack",
    "start": "next start",
    "lint": "next lint",
    "prisma-migrate-build": "dotenv -e .env.production -- npx prisma migrate deploy",
    "prisma-migrate:dev": "dotenv -e .env.development -- prisma migrate dev --name init",
    "prisma-migrate:prod": "dotenv -e .env.production -- prisma migrate dev --name init",
    "prisma-generate:dev": "dotenv -e .env.development -- npx prisma generate",
    "prisma-generate:prod": "dotenv -e .env.production -- npx prisma generate",
    "prisma:dev": "dotenv -e .env.development -- prisma",
    "prisma:prod": "dotenv -e .env.production -- prisma",
    "prisma:studio:dev": "dotenv -e .env.development -- prisma studio",
    "prisma:studio:prod": "dotenv -e .env.production -- prisma studio",
    "build": "dotenv -e .env.production -- prisma generate && dotenv -e .env.production -- node --loader ts-node/esm ./bin/build.ts",
    "build-dev": "dotenv -e .env.production -- prisma generate && dotenv -e .env.production -- node --loader ts-node/esm ./bin/build.ts --dev",
    "next-build": "next build"
  },
  "dependencies": {
    "@prisma/adapter-mariadb": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "classnames": "^2.5.1",
    "material-symbols": "^0.31.2",
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "sass": "^1.87.0"
  },
  "devDependencies": {
    "@alicloud/credentials": "^2.4.3",
    "@alicloud/ecs20140526": "7.0.6",
    "@alicloud/openapi-client": "^0.4.14",
    "@alicloud/tea-console": "^1.0.0",
    "@alicloud/tea-typescript": "^1.7.1",
    "@alicloud/tea-util": "^1.4.9",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.17.32",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "prisma": "^7.2.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "typescript": "^5"
  }
}
