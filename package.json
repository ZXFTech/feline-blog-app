{
  "name": "feline-blog-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "ts-node": {
    "esm": true
  },
  "scripts": {
    "dev": "dotenv -e .env.development -- next dev --turbopack",
    "start": "next start",
    "lint": "eslint",
    "fix": "eslint --fix",
    "build": "next build",
    "prisma-migrate-build": "dotenv -e .env.production -- npx prisma migrate deploy",
    "prisma-migrate:dev": "dotenv -e .env.development -- prisma migrate dev --name init",
    "prisma-migrate:prod": "dotenv -e .env.production -- prisma migrate dev --name init",
    "prisma-generate:dev": "dotenv -e .env.development -- npx prisma generate",
    "prisma-generate:prod": "dotenv -e .env.production -- npx prisma generate",
    "prisma:dev": "dotenv -e .env.development -- prisma",
    "prisma:prod": "dotenv -e .env.production -- prisma",
    "prisma-studio:dev": "dotenv -e .env.development -- prisma studio",
    "prisma-studio:prod": "dotenv -e .env.production -- prisma studio",
    "build-local:dev": "dotenv -e .env.development -- prisma generate && dotenv -e .env.development -- node --loader ts-node/esm ./bin/build.ts --dev",
    "build-local:prod": "dotenv -e .env.production -- prisma generate && dotenv -e .env.production -- node --loader ts-node/esm ./bin/build.ts",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@prisma/adapter-mariadb": "^7.2.0",
    "@prisma/client": "^7.3.0",
    "bcryptjs": "^3.0.3",
    "classnames": "^2.5.1",
    "cookie": "^1.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.562.0",
    "material-symbols": "^0.31.2",
    "next": "16.1.3",
    "next-auth": "^4.24.13",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-virtuoso": "^4.18.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "sass": "^1.87.0"
  },
  "devDependencies": {
    "@alicloud/credentials": "^2.4.3",
    "@alicloud/ecs20140526": "7.0.6",
    "@alicloud/openapi-client": "^0.4.14",
    "@alicloud/tea-console": "^1.0.0",
    "@alicloud/tea-typescript": "^1.7.1",
    "@alicloud/tea-util": "^1.4.9",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.17.32",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "^16.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prisma": "^7.3.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "typescript": "^5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "prisma"
    ]
  }
}
